{
    "name": "mnabialek/laravel-sql-logger",
    "description": "Log SQL queries in Laravel/Lumen framework",
    "keywords": ["laravel", "sql", "log", "logger", "lumen", "sql logger", "log sql queries", "log sql", "sql log"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Nabia≈Çek",
            "email": "dev@nabialek.org"
        }
    ],
    "require": {
        "illuminate/support": "5.*|6.*|7.*",
        "mnabialek/laravel-version": "^1.0.3",
        "nesbot/carbon": "~1.0 || ^2.0",
        "illuminate/filesystem": "5.*|6.*|7.*",
        "illuminate/container": "5.*|6.*|7.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.5",
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.0",
        "laravel/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mnabialek\\LaravelSqlLogger\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Mnabialek\\LaravelSqlLogger\\Tests\\": "tests"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mnabialek\\LaravelSqlLogger\\Providers\\ServiceProvider"
            ]
        },
        "thanks": {
            "name": "mnabialek/laravel-version",
            "url": "https://github.com/mnabialek/laravel-version"
        },
        "suggest": {
            "symfony/thanks": "Star packages you use running 'composer thanks' command"
        }
    }
}
